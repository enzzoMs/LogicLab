{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "baseUrl": ".",
  "paths": {
    "@components/*": ["src/components/*"],
    "@assets/*": ["src/assets/*"],
    "@layouts/*": ["src/layouts/*"],
    "@pages/*": ["src/pages/*"]
  },
  "compilerOptions": {
    "plugins": [
      {
        "name": "@astrojs/ts-plugin"
      }
    ]
  }
}
